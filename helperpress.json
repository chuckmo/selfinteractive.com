{
  "theme_path": "wp-theme",
  "css_path": "wp-theme/_src/sass/style.scss",
  "build_cmd": "gulp production",
  "build_cmd_dir": "./",
  "environments": {
    "development": {
      "migrate_db_method": "ssh"
    }
  },
  "wp": {
    "theme": {
      "slug": "selfinteractive",
      "name": "SELF Interactive",
      "desc": "A custom-built WordPress theme."
    },
    "plugins": [
      "wordpress-seo",
      "duplicate-post",
      "regenerate-thumbnails",
      "gist-github-shortcode"
    ]
  }
}